{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["standard", "plugin:vue/base"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-console": "off",
        "comma-dangle": ["error", "always-multiline"]
    }
}
